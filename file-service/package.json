{
  "name": "file-service",
  "version": "0.0.1",
  "description": "File Service Microservice for DropHub",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:cache": "jest src/cache/cache.service.spec.ts",
    "test:file-service": "jest src/modules/file/file.service.spec.ts",
    "test:download-service": "jest src/modules/file/services/download/download.service.spec.ts",
    "test:permission": "jest src/modules/permission-client/permission-client.service.spec.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.882.0",
    "@aws-sdk/s3-request-presigner": "^3.882.0",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/schedule": "^6.1.0",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "bullmq": "^5.66.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "mongoose": "^8.18.0",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "eslint": "^9.18.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/$1",
    "^@modules/(.*)$": "<rootDir>/modules/$1",
    "^@common/(.*)$": "<rootDir>/common/$1",
    "^@config/(.*)$": "<rootDir>/config/$1",
    "^@constants/(.*)$": "<rootDir>/constants/$1",
    "^@utils/(.*)$": "<rootDir>/utils/$1",
    "^@types/(.*)$": "<rootDir>/types/$1",
    "^@cache/(.*)$": "<rootDir>/cache/$1",
    "^@workers/(.*)$": "<rootDir>/workers/$1"
  }
}
